doctype html
html
    head
        title trash
        link(rel='stylesheet', href='/css/trash.css')
    body
        div
            #info
                #title
                    img.image(src = trash.title)
                #type 
                    h1 Type
                    p #{trash.type}
                #process 
                    h1 Process
                    p #{trash.process}
            #pic
                img.image(src = trash.pic)
